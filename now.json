{
  "version": 2,
  "name": "photo-album",
  "routes": [
    {
      "src": "/albums/(?<slug>[^/]*)",
      "dest": "/client/album?slug=$slug"
    },
    {
      "src": "/albums/(?<slug>[^/]*)/(?<filename>[^/]*)",
      "dest": "/client/photo?slug=$slug&filename=$filename"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "builds": [
    {
      "src": "client/next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ]
}
