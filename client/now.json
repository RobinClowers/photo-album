{
  "version": 2,
  "name": "photo-album",
  "alias": "photos-beta.robinclowers.com",
  "routes": [
    {
      "src": "/albums/(?<slug>[^/]*)",
      "dest": "/album?slug=$slug"
    },
    {
      "src": "/albums/(?<slug>[^/]*)/(?<filename>[^/]*)",
      "dest": "/album?slug=$slug&filename=$filename"
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ]
}
